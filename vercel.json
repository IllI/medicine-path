{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "app/.next",
  "framework": "nextjs",
  "rewrites": [
    { "source": "/studio", "destination": "/studio/" },
    { "source": "/studio/(.*)", "destination": "/studio/$1" }
  ],
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./app ./sanity || exit 1"
} 