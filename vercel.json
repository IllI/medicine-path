{
  "version": 2,
  "builds": [
    {
      "src": "app",
      "use": "@vercel/next"
    },
    {
      "src": "sanity",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/studio",
      "dest": "/studio/"
    },
    {
      "src": "/studio/(.*)",
      "dest": "/sanity/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_SANITY_PROJECT_ID": "11w5w28q",
    "NEXT_PUBLIC_SANITY_DATASET": "production",
    "NEXT_PUBLIC_SANITY_API_VERSION": "2023-05-03"
  }
} 